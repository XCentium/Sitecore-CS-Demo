<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>


    <cartServiceProvider type="KeefePOC.Pipelines.Providers.CartServiceProvider, KeefePOC" singleInstance="true" patch:instead="*[@type='Sitecore.Commerce.Services.Carts.CartServiceProvider, Sitecore.Commerce']"  />
  
    <pipelines>
      
      <keef.commerce.carts.validateCartLine>
        <processor type="KeefePOC.Pipelines.Validations.AddToCart.InmateProductValidation, KeefePOC" />
        <processor type="CSDemo.Pipelines.Validations.AddToCart.ProductGroupValidation, CSDemo" />
      </keef.commerce.carts.validateCartLine>     

		
		<keef.commerce.carts.viewcartvalidation>
			<processor type=" KeefePOC.Pipelines.Validations.ViewCart.CheckPromo, KeefePOC" />
		</keef.commerce.carts.viewcartvalidation>

	</pipelines>
  </sitecore>
</configuration>